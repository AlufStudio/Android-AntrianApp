<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.abdymalikmulky.settingqueue.app.ui.setting.SettingActivity">


    <TextView
        android:id="@+id/tv_setting_result"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_alignParentStart="true"
        android:layout_alignParentTop="true"
        android:text="@string/no_setting"
        android:gravity="left"/>

    <Button
        android:id="@+id/btn_setting"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_below="@+id/tv_setting_result"
        android:layout_centerHorizontal="true"
        android:text="@string/btn_new_setting"
        android:onClick="setting()"/>

</RelativeLayout>
